<div class="social-pages">
	{% for page in social_pages %}
		{% if page.url|length %}
			<a href="{{ page.url }}" {{ page.title ? 'title="' ~ page.title ~ '"' }} target="_blank" rel="me" class="u-url"><span class="fa fa-2x fa-{{ page.icon }}" title="{{ page.title }}"></span></a>
		{% endif %}
	{% endfor %}
</div>
